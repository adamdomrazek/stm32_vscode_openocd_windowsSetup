{
    "configurations": [
        {
            "name": "stm32 F4",
            "includePath": [
                "${workspaceFolder}/**"
            ],

            "defines": [
                "_DEGUG",
                "UNICODE",
                "_UNICODE",
                // z Makefile C_DEFS
                "USE_HAL_DRIVER",
                "STM32F429xx"
            ],
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "${env:ARMGCC_DIR}/arm-none-eabi-gcc.exe",
            // z Makefile "CFLAGS"
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}